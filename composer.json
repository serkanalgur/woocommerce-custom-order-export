{
  "name": "wexport/wexport",
  "description": "Export WooCommerce orders to CSV/XLSX with product-level custom codes",
  "version": "1.7.0",
  "type": "wordpress-plugin",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "Serkan Algur",
      "homepage": "https://serkanalgur.com.tr"
    }
  ],
  "require": {
    "php": ">=7.4",
    "composer/installers": "~1.0",
    "phpoffice/phpspreadsheet": "^1.29"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "wp-coding-standards/wpcs": "^3.0.0",
    "squizlabs/php_codesniffer": "^3.6.0"
  },
  "autoload": {
    "psr-4": {
      "WExport\\": "includes/",
      "WExport\\Admin\\": "admin/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "phpcs": "phpcs --standard=WordPress --extensions=php .",
    "phpcbf": "phpcbf --standard=WordPress --extensions=php ."
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
